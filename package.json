{
  "name": "vite-plugin-uni-pages-types",
  "version": "1.0.1",
  "description": "动态生成 uni-app 的 pages.json 的类型文件 pages.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lei1248276/vite-plugin-uni-pages-types.git"
  },
  "author": "Jaye",
  "keywords": [
    "uni-app",
    "uni-pages",
    "uni-pages-types"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^22.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.0.3"
  },
  "dependencies": {
    "chalk": "^5.3.0"
  }
}
